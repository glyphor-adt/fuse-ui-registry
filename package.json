{
  "name": "fuse-ui-registry",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter preview dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "aceternity:pull": "node scripts/pull-aceternity.mjs --cwd apps/preview",
    "aceternity:pull:ui": "node scripts/pull-aceternity-into-ui-package.mjs",
    "aceternity:pull:ui:write": "node scripts/pull-aceternity-into-ui-package.mjs --write --updateDeps"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  }
}
